generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User{
  id        Int     @default(autoincrement())   @id
  email     String  @unique
  username  String  @unique
  password  String
  name      String 

  events     Event[]
}

model Event{
  id        Int     @default(autoincrement()) @id
  title     String
  date      DateTime
  location  String
  description String
  createdById Int

  createdBy   User    @relation(fields: [createdById], references: [id])

}

